    <div class="center">
        <br />
        <table class="table" style="margin-left: 20px;">
            <thead>
                <tr>
                    <th scope="col">Zone ID</th>
                    <th scope="col">zone type</th>
                    <th scope="col">zone price</th>

                </tr>
                {{#zones}}
                <tr>
                    <td>{{id}}</td>
                    <td>{{zonetype}}</td>
                    <td>{{price}}</td>
                    <td>
                </tr>
                {{/zones}}
            </thead>
        </table>
</body>

</html>

